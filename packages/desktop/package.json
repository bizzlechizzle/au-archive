    "exiftool-vendored": "^33.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "kysely": "^0.27.2",
    "leaflet": "^1.9.4",
    "node-postal": "^1.3.0",
    "puppeteer-core": "^24.31.0",
    "sharp": "^0.33.1",
    "supercluster": "^8.0.1",
    "utf-8-validate": "^6.0.5",
    "zod": "^3.22.4"

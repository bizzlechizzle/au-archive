{
  "name": "@au-archive/desktop",
  "version": "0.1.0",
  "type": "module",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "postinstall": "electron-rebuild -f -o better-sqlite3,sharp",
    "dev": "vite",
    "build": "vite build",
    "build:electron": "electron-builder --config electron-builder.config.json",
    "build:all": "pnpm run build && pnpm run build:electron",
